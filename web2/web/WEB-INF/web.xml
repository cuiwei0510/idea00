<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>MySvt</servlet-name>
        <servlet-class>svt.MySvt</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginSvt</servlet-name>
        <servlet-class>svt.LoginSvt</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MySvt</servlet-name>
        <url-pattern>/MySvt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginSvt</servlet-name>
        <url-pattern>/LoginSvt</url-pattern>
    </servlet-mapping>
</web-app>